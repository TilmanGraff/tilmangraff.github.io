	date = {2018},
	langid = {english},
	file = {2018 - Ongoing primary forest loss in Brazil, Democratic .pdf:/Users/tilmangraff/Zotero/storage/8VHC6BYM/2018 - Ongoing primary forest loss in Brazil, Democratic .pdf:application/pdf}
