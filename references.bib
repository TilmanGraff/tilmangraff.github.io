@article{santamaria_reshaping_nodate,
	title = {Reshaping Infrastructure: Evidence from the division of Germany},
	abstract = {Can governments adjust transportation infrastructure to unexpected economic changes? This paper studies how the ability to reshape the highway network after a large economic shock, the division of Germany, impacted aggregate welfare. To understand the incentives to build infrastructure, I develop a multi-region quantitative trade model with endogenous infrastructure choice and calibrate it to the prewar German economy. I exploit the division of Germany, an exogenous change in borders, to test the ability of the model to predict highway development before and after the division. Using newly collected data, I document that the West German government considerably reshaped the highway network after the division shock. Reshaping the network after the division increased real income by 0.7\% to 2\%. However, this reshaping was constrained by the part of the network developed before division. Using the model, I quantify the cost of path-dependence from these pre-division highway links. I ﬁnd that the ability to reshape the full network could have increased real income by an additional 1.85\%, relative to the eﬃcient constrained network.},
	pages = {80},
